{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Ticket",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "institution": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::institution.institution"
    },
    "beneficiary": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::beneficiary.beneficiary"
    },
    "zone": {
      "type": "integer"
    },
    "address": {
      "type": "text"
    },
    "evidence": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "priority": {
      "type": "component",
      "repeatable": false,
      "component": "tickets.priority"
    },
    "state": {
      "type": "component",
      "repeatable": false,
      "component": "tickets.state"
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
